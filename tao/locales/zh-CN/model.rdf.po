msgid "Model Status"
msgstr "模型状态"

# http://www.tao.lu/Ontologies/TAO.rdf#ModelStatusStable
msgctxt "http://www.w3.org/2000/01/rdf-schema#label"
msgid "Stable"
msgstr "稳定"

# http://www.tao.lu/Ontologies/TAO.rdf#ModelStatusStable
msgctxt "http://www.w3.org/2000/01/rdf-schema#comment"
