	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
	xmlns:widget="http://www.tao.lu/datatypes/WidgetDefinitions.rdf#" 
>
  <rdf:Description rdf:about="http://www.tao.lu/Ontologies/TAO.rdf#OauthConsumer">
