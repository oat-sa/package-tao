                <xs:element ref="statsOperator" minOccurs = "1" maxOccurs = "1"/>
                <xs:element ref="mathOperator" minOccurs = "1" maxOccurs = "1"/>
            </xs:choice>
        </xs:sequence>
        <xs:attributeGroup ref="caseSensitive.StringMatch.Attr"/>
