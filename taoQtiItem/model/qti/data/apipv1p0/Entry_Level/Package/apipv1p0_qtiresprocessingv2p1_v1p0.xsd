                <xs:element ref="numberCorrect" minOccurs = "1" maxOccurs = "1"/>
                <xs:element ref="numberPresented" minOccurs = "1" maxOccurs = "1"/>
                <xs:element ref="numberResponded" minOccurs = "1" maxOccurs = "1"/>
